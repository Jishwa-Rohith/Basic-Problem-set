import java.util.*;
public class Testing_program_file {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        String fn = obj.next();
        String ln = obj.next();
        int n = obj.nextInt();

        for(int i=1 ; i<=n ; i++){
            System.out.println(fn+ln);
        }
    }
}
